@import"/iconfont.wxss";
.table {
  width: 100%;
  margin-bottom: 200rpx;
}
.tr {
  width: 100%;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #dadada;
}
.th,.td {
  padding:20rpx 0;
  text-align: center;
  width:100%;
  flex: 2;
}
.th {
  font-weight: 400;
  background-color: #dfdfdf;
  color: #222222;
  font-size: 28rpx;
}
.td{
  color: #484848;
  font-size: 24rpx;
  max-width: 115rpx;
  white-space:nowrap;
  text-overflow:ellipsis;
  display: block;
  overflow: hidden;
}
.t-date{
  flex: 3;
  max-width: 173rpx;
  white-space:nowrap;
  text-overflow:ellipsis;
  display: block;
  overflow: hidden;
}
.picker-wrapper{
  margin:40rpx 0;
}
.picker-name{
  margin-right: 20rpx;
  color: #484848;
}
.title-icon{
  color: #222222;
  font-size: 20rpx;
}